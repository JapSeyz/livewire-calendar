{
    "name": "japseyz/livewire-calendar",
    "description": "Laravel Livewire calendar component",
    "keywords": [
        "japseyz",
        "livewire-calendar"
    ],
    "homepage": "https://github.com/japseyz/livewire-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jesper Jacobsen",
            "email": "japseyz@japseyz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "livewire/livewire": "^3.4"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "JapSeyz\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JapSeyz\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JapSeyz\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ],
            "aliases": {
                "LivewireCalendar": "JapSeyz\\LivewireCalendar\\LivewireCalendarFacade"
            }
        }
    }
}
